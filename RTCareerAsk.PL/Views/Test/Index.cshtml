@model IEnumerable<BugModel>

<h2>@ViewBag.Title</h2>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>标题</th>
            <th>报告人</th>
            <th>优先级</th>
            <th>状态</th>
            <th></th>
        </tr>
    </thead>
    <tbody id="bugAccordion">
        @foreach (BugModel bug in Model)
        {
            @Html.Partial("_BugInfoForm", bug)
        }
    </tbody>
</table>